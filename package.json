{
  "name": "archtref-reactjs-typescript",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.5.0",
    "@types/jest": "^26.0.16",
    "@types/node": "^14.14.10",
    "@types/react-dom": "^17.0.0",
    "axios": "^0.21.0",
    "connected-react-router": "^6.8.0",
    "customize-cra": "^1.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "seamless-immutable": "^7.1.4",
    "source-map-explorer": "^2.5.1",
    "styled-components": "^5.2.1",
    "typescript": "4.1.0",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "prettier": "prettier --write . \"./src/**/*.{ts,tsx,js,jsx}\"",
    "lint": "./node_modules/.bin/eslint --fix \"./src/**/*.{ts,tsx,js,jsx}\"",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --coverage --watchAll false",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/seamless-immutable": "^7.1.15",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "babel-plugin-root-import": "^6.6.0",
    "eslint": "^7.15.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.4",
    "jest-canvas-mock": "^2.3.0",
    "prettier": "^2.2.1",
    "react-app-rewired": "^2.1.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier && npm run lint",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "jest": {
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "moduleNameMapper": {
      "\\.(woff2?|ttf|otf|eot|png|jpe?g|gif|svg|css|scss)$": "<rootDir>/mock/jest-mockfile.js",
      "^~\\/(.*)$": "<rootDir>/src/$1"
    },
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover",
      "html"
    ],
    "collectCoverageFrom": [
      "**/*.{tsx,jsx,ts}"
    ],
    "coveragePathIgnorePatterns": [
      "reportWebVitals.ts",
      "\\.d\\.ts$",
      "/node_modules/",
      "/src/styles/",
      "/src/assets/",
      "/src/services/",
      "/src/index.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    }
  }
}
